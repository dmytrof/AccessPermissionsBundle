{
    "name": "dmytrof/access-permissions-bundle",
    "type": "symfony-bundle",
    "description": "Symfony AccessPermissionsBundle",
    "keywords": ["Security", "Access rights", "Permissions"],
    "license": "MIT",
    "authors": [
        {
            "name": "Dmytro Feshchenko",
            "email": "dmytro.feshchenko@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2 || ^8.0",
        "symfony/framework-bundle": "^4.3 || ^5.0",
        "doctrine/orm": "^2.6",
        "doctrine/inflector": "^2.0",
        "symfony/security-bundle": "^4.3 || ^5.0",
        "symfony/validator": "^4.3 || ^5.0",
        "symfony/form": "^4.3 || ^5.0",
        "symfony/translation": "^4.3 || ^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3 || ^9.1"
    },
    "autoload": {
        "psr-4": {
            "Dmytrof\\AccessPermissionsBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dmytrof\\AccessPermissionsBundle\\Tests\\": "tests/"
        }
    },
    "suggest": {
        "symfony/security": "Adds access permissions for administrator for entities and other resources"
    },
    "scripts": {
        "test": "phpunit"
    }
}
